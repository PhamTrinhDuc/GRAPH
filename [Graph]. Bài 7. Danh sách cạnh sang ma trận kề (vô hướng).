#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;
int a[1005][1005] ; 

void inp(){
    int n , m ; cin >> n >> m ; 
    for(int i=0 ; i<m ; i++ ){
        int x , y ; cin >> x >> y ; 
        a[x][y] = 1 ; a[y][x]=1 ;
    }
    for(int i=1 ; i<=n ; i++ ){
        for(int j=1 ; j<=n ; j++ ) cout << a[i][j] <<' ' ; 
        cout << endl ; 
    }
}

int main() {
    inp() ; 
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
